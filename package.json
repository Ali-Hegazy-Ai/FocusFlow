{
  "name": "focusflow",
  "private": true,
  "version": "0.1.0",
  "type": "module",  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:prod": "npm run clean && npm run build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "clean": "if exist dist rmdir /s /q dist && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite",
    "typecheck": "tsc --noEmit",
    "deploy:netlify": "npm run build:prod && npx netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build:prod && npx vercel --prod"
  },
  "dependencies": {
    "framer-motion": "^10.16.4",
    "howler": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "terser": "^5.42.0",
    "vite": "^4.4.5"
  }
}
